#/bin/bash

MY_DIR=$( dirname "${BASH_SOURCE[0]}" )

ESCAPED_TAG_VERSION=${GITHUB_SHA//\//-}
UPSTREAM_TAG=585172581592.dkr.ecr.ap-southeast-2.amazonaws.com/vly-alpha:${ESCAPED_TAG_VERSION}

# list out all our variables
echo BUILD_REVISION=${REVISION} \
    BUILD_APP_URL=${APP_URL} \
    BUILD_AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID} \
    BUILD_AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY} \
    BUILD_VLY_PRIVATE_KEY="${VLY_PRIVATE_KEY}" \
    BUILD_SMTP_ID=${SMTP_ID} \
    BUILD_BADGR_USERNAME=${BADGR_USERNAME}\
    BUILD_BADGR_PASSWORD=${BADGR_PASSWORD}\
    BUILD_SMTP_PWD=${SMTP_PWD} \


echo voluntarily/vly2:${ESCAPED_TAG_VERSION} ${UPSTREAM_TAG}

printenv